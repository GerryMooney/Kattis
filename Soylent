import java.util.*;
public class Soylent{
    public static void main(String args[]){
        Scanner nuts=new Scanner(System.in);
        int a=nuts.nextInt();
        for(int i=0;i<a;i++){
            double b=nuts.nextInt();
            System.out.println((int)Math.ceil(b/400.0));//math.ciel rounds to the nearest greater number unlike math.round
        }
}
}
