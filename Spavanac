import java.util.*;
public class Spavanac{
    public static void main(String args[]){
    Scanner nuts=new Scanner(System.in);
    int h=nuts.nextInt();
    int m=nuts.nextInt();
    int min=m-45;
    if(min>0){
        System.out.println(h+" "+min);
    }
    else{
        if(h==0){
            h=24;
        }
        h--;
        m=min+60;
        System.out.println(h+" "+m);
    }
    }
}
